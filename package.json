{
  "//": "The production flag is added in the jenkins build script.",
  "name": "lms-sync-courses",
  "version": "0.1.0",
  "description": "A program which propagates courses into Canvas LMS.",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production nodemon app.js",
    "start-dev": "cross-env NODE_ENV=development nodemon app.js",
    "test": "tape \"test/unit/**/*.js\" | tap-spec",
    "test:integration": "tape \"test/integration/**/*.js\" | tap-spec",
    "test:docker-unit": "npm install --no-optional; npm test",
    "test:docker-integration": "npm install --no-optional; npm run test:integration",
    "debug": "NODE_ENV=development node --nolazy --inspect-brk=9229 app.js | bunyan -o short"
  },
  "author": "team-e-larande",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KTH/lms-sync-courses.git"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "dotenv": "^5.0.1",
    "express": "^4.16.4",
    "kth-canvas-api": "^0.15.7",
    "kth-canvas-utilities": "^1.18.0",
    "kth-node-server": "^3.1.1",
    "ldapjs": "^1.0.2",
    "lodash": "^4.17.11",
    "node-schedule": "^1.3.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "fs-extra": "^6.0.1",
    "nodemon": "^1.18.10",
    "proxyquire": "^2.1.0",
    "rewire": "^4.0.1",
    "sinon": "^5.1.1",
    "standard": "^11.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1"
  }
}
