{
  "//": "The production flag is added in the jenkins build script.",
  "name": "lms-sync-courses",
  "version": "0.1.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production nodemon app.js",
    "start-dev": "cross-env NODE_ENV=development nodemon app.js",
    "test": "tape \"test/unit/**/*.js\" | tap-spec",
    "test:integration": "cross-env NODE_ENV=development tape \"test/integration/**/*.js\" | tap-spec",
    "test:docker-unit": "npm install --no-optional; npm test",
    "test:docker-integration": "npm install --no-optional; npm run test:integration"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "kth-canvas-api": "^0.15.5",
    "kth-canvas-utilities": "^1.17.2",
    "kth-node-server": "^3.1.1",
    "ldapjs": "^1.0.2",
    "node-schedule": "^1.3.0",
    "request": "^2.86.0",
    "request-promise": "^4.2.2"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "cross-env": "^5.1.5",
    "fs-extra": "^6.0.1",
    "nodemon": "^1.17.4",
    "proxyquire": "^2.0.1",
    "rewire": "^4.0.1",
    "sinon": "^5.0.7",
    "standard": "^11.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0"
  }
}
